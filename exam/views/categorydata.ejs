<%- include('./partials/header') %>
    <%- include('./partials/sidebar') %>
        <div class="page-wrapper">
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <a href="/categorylist"></a>
                            <div class="card-body">
                                <h5 class="card-title m-b-0">Static Table</h5>
                            </div>
                            <% if(details!=null) { %>
                            <table class="table">
                                <thead>
                                    <tr align="center">
                                        <th scope="col">ID</th>
                                        <th scope="col">Category Name</th>
                                        <th scope="col">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% details.forEach(function(item,index){  %>
                                    <tr align="center">
                                        <td><%= index + 1 %></td>
                                        <td><%= item.categoryname%></td>
                                        <td>
                                            <a class="btn btn-success" href="/catedit?id=<%= item._id%>">Edit</a> 
                                            <a class="btn btn-danger"  href="/catdelete/<%= item._id%>">Delete</a>
                                        </td>
                                    </tr>
                                </tbody>
                                <% }) %>
                            </table>
                            <% } %>
                        </div>
                    </div>

                    <!-- ============================================================== -->
                    <!-- End PAge Content -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- Right sidebar -->
                    <!-- ============================================================== -->
                    <!-- .right-sidebar -->
                    <!-- ============================================================== -->
                    <!-- End Right sidebar -->
                    <!-- ============================================================== -->
                </div>
            </div>
            <%- include('./partials/footer') %>