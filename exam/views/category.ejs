                                        <label for="lname" class="col-sm-3  control-label col-form-label">Category Name</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="categoryname" class="form-control" id="lname"
                                                placeholder="Enter Category Name" value="<%= data.categoryname %>">
                                                
                                        </div>
                                    </div>
                                </div>
                                <div class="border-top">
                                    <div class="card-body">
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                      
